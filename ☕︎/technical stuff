Useful Command Prompt Commands.txt

C:\>

1.
	tree
	tree /?
	tree | clip
2.
	netsh wlan show profile
	netsh wlan show profile [NAME] key=clear
3. systeminfo
4.
	assoc
	assoc [.filetype]
	assoc [.filetype]=[SOFTWARE][.software]
5. fc [FILENAME1][.filetype1] [FILENAME2][.filetype2]
6.
	color /?
	color [number][LETTER]
7.
	prompt [PROMPT_NAME]:
	[PROMPT_NAME]:title [WINDOW_NAME]
8. explorer .
9. doskey/history
10. curl qrenco.de/https://www.[WEBSITE_NAME].com
11.
	shutdown /s /t [SECONDS]
	shutdown /a
12.
	tasklist
	taskkill /PID [PID_NUMBER] /F
13.
	ipconfig
	ipconfig/all
	ping [WEBSITE_NAME].com
	netstat
	getmac/v
14.
	powercfg /energy

	powercfg /batteryreport

